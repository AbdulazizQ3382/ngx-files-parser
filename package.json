{
    "name": "open-data-frontend",
    "version": "0.0.0",
    "scripts": {
      "ng": "ng",
      "start": "ng serve",
      "build": "ng build",
      "watch": "ng build --watch --configuration development",
      "test": "ng test",
      "dev:ssr": "ng run OpenData-Frontend:serve-ssr",
      "serve:ssr": "node dist/OpenData-Frontend/server/main.js",
      "build:ssr-dev": "ng build --configuration development && ng run OpenData-Frontend:server:development",
      "build:ssr-preprod": "ng build --configuration preproduction && ng run OpenData-Frontend:server:preproduction",
      "build:ssr-prod": "ng build --configuration production && ng run OpenData-Frontend:server:production",
      "prerender": "ng run OpenData-Frontend:prerender",
      "build:ssr": "ng build && ng run OpenData-Frontend:server",
      "upgrade": "ng update"
    },
    "private": true,
    "dependencies": {
      "ngx-csv-parser": "^1.2.2",
      "stream-browserify": "^3.0.0",
      "timers-browserify": "^2.0.12",
      "xlsx": "https://cdn.sheetjs.com/xlsx-0.19.3/xlsx-0.19.3.tgz",
      "xml2js": "^0.5.0"
    },
    "devDependencies": {
    }
  }
  